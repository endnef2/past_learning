    "def solution(nums):\n",
    "    num = set(nums)\n",
    "    div = (len(nums)//2)\n",
    "    if div < len(num) :\n",
    "        answer = div\n",
    "    else :\n",
    "        answer = len(num)\n",
    "    return answer"
   ]
