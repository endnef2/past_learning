  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(arr):\n",
    "    sarr = 0\n",
    "    for i in range(0, len(arr)):\n",
    "        sarr += arr[i]\n",
    "    answer = sarr/(len(arr))\n",
    "    return answer"
   ]
  },
