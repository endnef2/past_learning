   "source": [
    "# 약수의 합\n",
    "## https://programmers.co.kr/learn/courses/30/lessons/12928"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solution(n):\n",
    "    div = []\n",
    "    answer = 0\n",
    "    for i in range(1, n+1) :\n",
    "        if n%i == 0 :\n",
    "            div.append(i)\n",
    "    for j in div :\n",
    "        answer += j\n",
    "        \n",
    "    return answer"
   ]
  },
